package flash.display {


/**
 * The StageDisplayState class provides values for the <code>Stage.displayState</code> property.
 * @see Stage#displayState
 *
 */
public final class StageDisplayState {
  /**
   * Specifies that the Stage is in full-screen mode. Keyboard interactivity is disabled in this mode.
   */
  public static const FULL_SCREEN:String = "fullScreen";
  /**
   * Specifies that the Stage is in normal mode.
   */
  public static const NORMAL:String = "normal";
}
}
