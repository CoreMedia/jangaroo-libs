package flash.display {


/**
 * The ActionScriptVersion class is an enumeration of constant values that indicate the language version of a loaded SWF file. The language version constants are provided for use in checking the <code>actionScriptVersion</code> property of a flash.display.LoaderInfo object.
 * @see LoaderInfo#actionScriptVersion
 *
 */
public final class ActionScriptVersion {
  /**
   * ActionScript language version 2.0 and earlier.
   */
  public static const ACTIONSCRIPT2:uint = 2;
  /**
   * ActionScript language version 3.0.
   */
  public static const ACTIONSCRIPT3:uint = 3;
}
}
